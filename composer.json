{
    "name": "ao/translation-bundle",
    "type": "symfony-bundle",
    "description": "Doctrine as symfony translation provider + a nice gui for editing translations.",
    "keywords": ["symfony", "doctrine", "translation", "i18n", "ui", "gui", "multilanguage", "interface", "webinterface", "wordings"],
    "homepage": "https://github.com/adrianolek/AOTranslationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Olek",
            "email": "adrianolek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/framework-bundle": "~2.1",
        "stof/doctrine-extensions-bundle": "1.1.*@dev"
    },
    "suggest": {
        "sonata-project/admin-bundle": "Use translations admin backend.",
        "sonata-project/doctrine-orm-admin-bundle": "Use translations admin backend."
    },
    "autoload":     {
        "psr-0": { "AO\\TranslationBundle": "" }
    },
    "target-dir": "AO/TranslationBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
